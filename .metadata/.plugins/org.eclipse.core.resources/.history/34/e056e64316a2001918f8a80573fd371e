
public class FixedArrayStack implements Stack {
	private Object[] stack;
	static int top = -1;
	FixedArrayStack(int size)
	{
		stack = new Object[size];
		top++;
	}

	@Override
	public void push(Object o) {
		// TODO Auto-generated method stub
		stack[top] = o;
	}

	@Override
	public Object pop() {
		// TODO Auto-generated method stub
		
		return stack[top--];
	}
	
}
