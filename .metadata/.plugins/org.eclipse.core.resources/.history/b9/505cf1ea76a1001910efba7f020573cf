class Circle
{
	private int radius;
	Circle(int radius)
	{
		this.radius = radius;
	}
	
	public String getShapeName()
	{
		return "Circle";
	}
	
	public double computeArea()
	{
		return 3.14*radius*radius;
	}
}


public class ShapesApp {
	Object[] shapes = new Object[10];
	public static void main(String[] args) {
		
	}
}
